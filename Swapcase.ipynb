   "source": [
    "def myfunc(word):\n",
    "    word = list(word)\n",
    "    j=\"\"\n",
    "    for i in range(len(word)):\n",
    "        if i % 2 == 0:\n",
    "            word[i] = word[i].upper()\n",
    "            j+=word[i]\n",
    "        else:\n",
    "            word[i]=word[i].lower()\n",
    "            j+=word[i]\n",
    "    return j"
   ]
